{
  "name": "vue3-antv-template",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "cross-env vite --mode development",
    "debug-vite": "cross-env vite --mode development --debug",
    "build:test": "cross-env vite build --mode test",
    "build:prod": "cross-env NODE_ENV=production vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "type-check": "vue-tsc --noEmit",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "format": "prettier --write src/",
    "clean-dev": "vite --force",
    "report": "rimraf dist && cross-env vite build",
    "swagger": "swagger-parser -f && npm run lint:eslint"
  },
  "dependencies": {
