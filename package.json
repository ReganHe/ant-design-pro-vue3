    "@vueuse/core": "^9.13.0",
    "ant-design-vue": "^4.0.0",
    "axios": "^0.24.0",
    "crypto-js": "4.1.1",
    "dayjs": "1.11.9",
    "less": "4.1.3",
    "less-loader": "11.1.0",
    "lodash-es": "^4.17.21",
    "lodash.clonedeep": "4.5.0",
    "lodash.debounce": "4.0.8",
    "lodash.pull": "4.1.0",
    "mitt": "3.0.0",
